	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Edge",
			"request": "launch",
			"type": "msedge",
			"url": "http://localhost:5500/phonespr.html",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "java",
			"name": "Launch App",
			"request": "launch",
			"mainClass": "com.example.App"
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\server\\server.js"
		}
	]
}